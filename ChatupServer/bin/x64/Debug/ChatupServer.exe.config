<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.runtime.remoting>
    <application name="Server">
      <service>
        <wellknown mode="Singleton" type="ChatupNET.Remoting.SessionService, ChatupServer" objectUri="session.rem" />
        <wellknown mode="Singleton" type="ChatupNET.Remoting.LobbyService, ChatupServer" objectUri="lobby.rem" />
      </service>
      <channels>
        <channel ref="tcp" port="12480">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>