<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown type="IMessageSingleton, Common" url="tcp://localhost:9002/Server/MessageServer"/>
        <wellknown type="IUserSingleton, Common" url="tcp://localhost:9002/Server/UserServer"/>
        <wellknown type="IChatroomSingleton, Common" url="tcp://localhost:9002/Server/ChatroomServer"/>
      </client>
      <channels>
        <channel ref="tcp" port="0">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>