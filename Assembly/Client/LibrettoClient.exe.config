<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="TcpBehaviorConf">
          <clientCredentials>
            <serviceCertificate>
              <authentication certificateValidationMode="None" revocationMode="NoCheck" />
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <bindings>
      <netTcpBinding>
        <binding name="TcpEndpoint">
          <security mode="Message">
            <message clientCredentialType="UserName" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:9000/LibrettoBookstore/"
                behaviorConfiguration="TcpBehaviorConf"
                binding="netTcpBinding"
                bindingConfiguration="TcpEndpoint"
                contract="StoreService.IStoreService"
                name="TcpEndpoint">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>